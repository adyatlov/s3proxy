{
  "id": "/s3proxy",
  "cmd": "./s3proxy $PORT0",
  "cpus": 1,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "env": {
    "AWS_ACCESS_KEY_ID": "XXXXXXXXXXXXXXXXXXXX",
    "AWS_SECRET_ACCESS_KEY": "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX"
  },
  "healthChecks": [
    {
      "protocol": "COMMAND",
      "command": {
        "value": "curl -f -X GET http://$HOST:$PORT0/s3-region/bucket-name/path/to/file.txt"
      },
      "gracePeriodSeconds": 300,
      "intervalSeconds": 300,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "portDefinitions": [
    {
      "protocol": "tcp",
      "labels": {
        "VIP_0": "3.3.3.3:80"
      }
    }
  ],
  "uris": [
    "https://github.com/adyatlov/s3proxy/releases/download/v1.0/s3proxy.zip"
  ]
}